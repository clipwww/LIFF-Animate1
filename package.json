{
  "name": "liff-anime",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run build-ts",
    "build-gp": "vue-cli-service build",
    "build-ts": "node_modules/typescript/bin/tsc --project ./server",
    "dev": "node_modules/nodemon/bin/nodemon.js",
    "heroku-postbuild": "npm run build",
    "nodemon-exec": "npm run build-ts && npm run serve",
    "start": "node ./dist/index.js",
    "report": "vue-cli-service build --report"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "date-fns": "^2.15.0",
    "element-ui": "^2.13.0",
    "express": "^4.17.1",
    "firebase": "^7.8.2",
    "helmet": "^3.21.2",
    "intro.js": "^2.9.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "tailwindcss": "^1.5.2",
    "vant": "^2.9.3",
    "vue": "^3.0.0-beta.1",
    "vue-analytics": "^5.22.1",
    "vue-meta": "^2.3.2",
    "vue-router": "^4.0.0-alpha.6",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuex": "^4.0.0-alpha.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/intro.js": "^2.4.6",
    "@types/lodash": "^4.14.149",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/compiler-sfc": "^3.0.0-beta.1",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.8.5",
    "nodemon": "^2.0.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-vue-next": "~0.1.3"
  },
  "engines": {
    "node": "12.13.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./server/**/*.ts"
    ],
    "exec": "npm run nodemon-exec",
    "ext": "ts"
  }
}
